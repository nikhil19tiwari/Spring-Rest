package in.pwskill.nikhil;

public class MaxMin {
public static void main(String []args) {
	int arr[] = {4,5,2,7,5,3,2};
	int max
	int min= 0;
   for(int i :arr) {
	   if(arr[i] > max)max = arr[i];
	   else min = arr[i];
   }
   System.out.print("the max is ::"+max);
   System.out.print("the min is ::"+min);
}
}
